<html>
  <body>
    <h1>Cities</h1>
    <table>
      <tr>
        <th>City code</th>
        <th>Country code</th>
        <th>City name</th>
        <th>Population</th>
        <th>Coastal</th>
        <th>Area (KM^2)</th>
      </tr>
<% cities.forEach((city)) => { %>
      <tr>
        <td><%=city.cty_code></td>
        <td><%=city.co_code</td>
        <td><%=city.cty_name</td>
        <td><%=city.population</td>
        <td><%=city.isCoastal</td>
        <td><%=city.areaKM</td>
        <td><a href="/update:city.cty_code">Update</a></td>
        <td><a href="/delete:city.cty_code">Delete</a></td>
      </tr>
<% }) %>
    </table>
  </body>
</html>