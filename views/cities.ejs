<html>
  <body>
    <h1>Cities</h1>
    <table>
      <tr>
        <th>City code</th>
        <th>Country code</th>
        <th>City name</th>
        <th>Population</th>
        <th>Coastal</th>
        <th>Area (KM^2)</th>
      </tr>
<% cities.forEach((city)) => { %>
      <tr>
        <td><%= city.cty_code %></td>
        <td><%= city.co_code %></td>
        <td><%= city.cty_name %></td>
        <td><%= city.population %></td>
        <td><%= city.isCoastal %></td>
        <td><%= city.areaKM %></td>
        <td><a href="/details:city.cty_code">All details</a></td>
      </tr>
<% }) %>
    </table>
    <a href="/">Home</a>
  </body>
</html>